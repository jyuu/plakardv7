.navbar-header {
    float: center;
    padding: 15px;
    text-align: center;
    width: 100%;
    font-family: helvetica;
    font-size: 13px;
    font-weight: lighter;
}
.hello{
  font-family: 'Homemade Apple';
  font-size: 15px;
}
.navbar-default .navbar-brand {
	float:none;
  font-family: 'Homemade Apple';
  font-size: 40px;
  color: #000;
}
.well-sm{
	background-color: rgba(255,255,255,.4);
	font-weight: bold;
	font-family: helvetica;
	text-align: left; 
	color: #333333;
	border: none;
}
.horizontalLine{
  border-top: thick solid #000000;
  border-width: 2px;
  border-top-style: double dotted;
}
.wrapper {
    min-height: 100%;
    height: auto !important;
    height: 100%;
    margin: 0 auto -60px; /* the bottom margin is the negative value of the footer's height */
}
.push{
  height: 60px;
}
.footer {
  
  padding:15px;
  font-weight: lighter;
	font-family: helvetica;
  width: 100%;
  /* Set the fixed height of the footer here */
  height: 60px;
  background-color: #f5f5f5;
}

.btn-primary{
  background-color: #B02051;
  border: none;
}
h2 {
  color: #B02051;
  font-weight:bold;
  text-transform: uppercase;
}

#guest-form {
  margin:10px 0;
  background: #FFE4E1;
  padding: 10px 10px 1px;
}



.table-striped>tbody>tr:nth-child(odd)>td,
.table-striped>tbody>tr:nth-child(odd)>th {
  background-color: #FFE4E1;
}

.table-striped>tbody>tr>td,
.table-striped>tbody>tr>th {
  font-family: helvetica;
}

body{
  font-family: helvetica;
  padding:0px;
  margin:0px;
}
p{
  font-family: helvetica;
  font-weight: lighter;

}
a{
  font-family: helvetica;
  color: #B02051;
  text-decoration: none
}
a:hover{
  color: #000000;
  text-decoration: none;
}
a:visited {
  color: #B02051;
    text-decoration: none;
}
.image-responsive {
width: 508px;
height: 618px;
padding-top: 318px;
opacity: 1;
}

img {
   padding:1px;
   margin:10px;
   border:1px solid #B02051;
   background-color:#FFE4E1;
}
